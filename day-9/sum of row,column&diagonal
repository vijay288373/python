matrix=[[1,2,3],[4,5,6],[7,8,9]]
row_sum=[sum(row) for row in matrix]
col_sum=[sum(matrix[i][j] for i in range(len(matrix))) for j in range(len(matrix))]
dig_sum=[sum(matrix[i][j] for i in range(len(matrix)))]
print("row_sum", row_sum)
print("column_sum", column_sum)
print("diagonal_sum", diagonal_sum)
